{
  "name": "bill-swift",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 refine dev",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 refine dev",
    "build": "refine build && rimraf .next/cache",
    "start:prod": "refine start",
    "prisma-generate": "dotenv -e .env.local -- prisma db pull && prisma-case-format --file ./prisma/schema.prisma && prisma generate",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate deploy",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.8.3",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.6",
    "@mui/x-data-grid": "^6.11.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.1.1",
    "@refinedev/cli": "^2.9.0",
    "@refinedev/core": "^4.36.0",
    "@refinedev/kbar": "^1.3.0",
    "@refinedev/mui": "^5.13.4",
    "@refinedev/nextjs-router": "^5.5.0",
    "@refinedev/react-hook-form": "^4.8.2",
    "@refinedev/simple-rest": "^4.5.0",
    "@t3-oss/env-nextjs": "^0.6.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "i18next": "^23.2.3",
    "lodash": "^4.17.21",
    "next": "^13.4.16",
    "next-i18next": "^14.0.0",
    "nookies": "^2.5.2",
    "prisma": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/parser": "^6.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.16",
    "prettier": "^3.0.2",
    "prisma-case-format": "^1.7.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
