{
  "name": "bill-swift",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=true NODE_OPTIONS=--max_old_space_size=4096 refine dev | pino-pretty -tc",
    "dev:e2e": "cross-env FORCE_COLOR=true NODE_OPTIONS=--max_old_space_size=4096 dotenv -e .env.test.local -- refine dev | pino-pretty -tc",
    "build": "prisma generate && refine build && rimraf .next/cache",
    "build:dev": "dotenv -e .env.dev -- npm run build",
    "build:stage": "dotenv -e .env.stage -- npm run build",
    "build:prod": "dotenv -e .env.prod -- npm run build",
    "start": "prisma migrate deploy && refine start",
    "prisma-generate": "dotenv -e .env.local -- prisma db pull --force && prisma-case-format --file ./prisma/schema.prisma && ts-node --project tsconfig.local.json prisma/replaceRelationField.ts && prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate deploy",
    "migrate-reset": "npx prisma migrate reset --force",
    "migrate-reset:local": "dotenv -e .env.local -- npx prisma migrate reset --force",
    "migrate-reset:test": "dotenv -e .env.test.local -- npx prisma migrate reset --force",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "test": "dotenv -e .env.test -- jest --runInBand",
    "test-watch": "dotenv -e .env.test -- jest --watch --runInBand",
    "test:e2e": "playwright test",
    "test:e2e-debug": "playwright test --debug",
    "test:e2e-codegen": "playwright codegen http://localhost:3000"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --project tsconfig.local.json prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.8.3",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.6",
    "@mui/x-data-grid": "^6.11.1",
    "@mui/x-date-pickers": "^6.16.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.1.1",
    "@refinedev/cli": "^2.16.4",
    "@refinedev/core": "^4.44.3",
    "@refinedev/kbar": "^1.3.4",
    "@refinedev/mui": "^5.13.15",
    "@refinedev/nextjs-router": "^5.5.2",
    "@refinedev/react-hook-form": "^4.8.10",
    "@refinedev/simple-rest": "^4.5.4",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "jose": "^4.15.2",
    "lodash": "^4.17.21",
    "next": "^13.4.16",
    "nookies": "^2.5.2",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "prisma": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.38.0",
    "@quramy/prisma-fabbrica": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/parser": "^6.4.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.16",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "^3.0.2",
    "prisma-case-format": "^1.7.2",
    "prisma-query-log": "^3.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
